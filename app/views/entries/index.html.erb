<div class="container">
    <div class="row">
            <div class="col-lg-2"></div>
            <div class="col-lg-8">
                <h3>応募履歴一覧</h3>
                <% @entry.each do |e| %>
                 <%= link_to entry_path(e) do %>
                    <div class="job_listing_table">
                        <table>
                            <tbody>
                                <tr>
                                   <td><%= e.contract_date.strftime('%Y年%m月%d日') %></td>
                               </tr>
                               <tr>
                                   <td><%= e.job_listing.company.company_name %></td>
                               </tr>
                               <tr>
                                   <td><%= e.job_listing.genre.genre %>
                               </tr>
                               <tr>
                               </tr>
                            </tbody>
                        </table>
                    </div>
                <% end %>
                <% end %>
            </div>
            <div class="col-lg-2"></div>
        </div>
</div>


